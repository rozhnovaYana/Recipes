// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Meal {
  id    Int     @id @default(autoincrement())
  slug String @unique
  title String
  image String
  summary String
  instructions String
  creator String
  creator_email String
}

